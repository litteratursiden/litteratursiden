{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "gdsmith/easing",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/gdsmith/jquery.easing/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery_validation/jqueryvalidate",
                "version": "1.17.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jquery-validation/jquery-validation/releases/download/1.17.0/jquery-validation-1.17.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ganlanyuan/tiny-slider",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/ganlanyuan/tiny-slider/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "^8.1",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "danskernesdigitalebibliotek/ddb-cover-service-php-client": "dev-master",
        "dinbror/blazy": "master",
        "drupal/advagg": "^6.0@alpha",
        "drupal/antibot": "^2.0",
        "drupal/block_exclude_pages": "^2.0",
        "drupal/bootstrap": "^3.5",
        "drupal/color_field": "^3.0",
        "drupal/config_filter": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/crop": "^2.1",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/email_registration": "^1.2",
        "drupal/entityqueue": "^1.2",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/extrafield_views_integration": "^2.0",
        "drupal/facets": "^3.0@beta",
        "drupal/field_group": "^3.2",
        "drupal/flag": "^4.0-alpha3",
        "drupal/flood_control": "^2.2",
        "drupal/group": "^1.0-rc1",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/jquery_ui": "^1.6",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/mailchimp": "^2.0",
        "drupal/mailsystem": "^4.1",
        "drupal/maxlength": "2.1",
        "drupal/memcache": "^2.1",
        "drupal/metatag": "^1.4",
        "drupal/override_node_options": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.2",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_users": "^3.0",
        "drupal/redirect404_home": "^2.0",
        "drupal/reroute_email": "^2.1",
        "drupal/rest_views": "^3.0@dev",
        "drupal/restui": "^1.15",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.23",
        "drupal/social_media": "^2.0",
        "drupal/swiftmailer": "^2.2",
        "drupal/system_status": "^2.8",
        "drupal/twig_tweak": "^3.2",
        "drupal/user_registrationpassword": "^2.0",
        "drupal/varnish_purge": "^2.1",
        "drupal/video_embed_field": "^2.5",
        "drupal/video_embed_html5": "^2.0",
        "drupal/view_unpublished": "^1.1",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/xmlsitemap": "^1.4",
        "drush/drush": "^10.0",
        "ganlanyuan/tiny-slider": "master",
        "gdsmith/easing": "master",
        "itk-dev/web_accessibility_statement": "^1.1",
        "jquery_validation/jqueryvalidate": "~1.0",
        "kenwheeler/slick": "master",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
      "drupal/core-dev": "^9.3",
      "drupal/upgrade_status": "^3.14",
      "mglaman/phpstan-drupal": "^1.1",
      "phpstan/extension-installer": "^1.2",
      "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
      "code-analysis/phpstan": [
        "phpstan analyse --configuration=phpstan.neon"
      ],
      "code-analysis": [
        "@code-analysis/phpstan"
      ],
      "coding-standards-check/phpcs": [
        "vendor/bin/phpcs --standard=phpcs.xml.dist"
      ],
      "coding-standards-check": [
        "@coding-standards-check/phpcs"
      ],
      "coding-standards-apply/phpcs": [
        "scripts/phpcbf --standard=phpcs.xml.dist"
      ],
      "coding-standards-apply": [
        "@coding-standards-apply/phpcs"
      ],
      "post-install-cmd": [
        "@install-codestandards"
      ],
      "install-codestandards": [
        "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
      ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
          "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "comment settings patch": "patches/core/comment_settings_tabs.patch",
                "invalid property": "patches/core/invalid_property.patch"
            },
            "drupal/image_widget_crop": {
                "Fix floating point issue": "https://www.drupal.org/files/issues/2020-06-22/3153736-2.patch"
            },
            "drupal/redirect404_home": {
                "https://www.drupal.org/project/redirect404_home/issues/3343349": "https://www.drupal.org/files/issues/2023-02-21/3343349-3.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/custom/*/composer.json"
            ]
        }
    }
}
