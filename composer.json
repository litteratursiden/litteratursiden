{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "master",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/kenwheeler/slick.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "master",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/dinbror/blazy.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "gdsmith/easing",
                "version": "master",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/gdsmith/jquery.easing.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery_validation/jqueryvalidate",
                "version": "1.17.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jquery-validation/jquery-validation/releases/download/1.17.0/jquery-validation-1.17.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ganlanyuan/tiny-slider",
                "version": "master",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/ganlanyuan/tiny-slider.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "danskernesdigitalebibliotek/ddb-cover-service-php-client": "dev-master",
        "dinbror/blazy": "master",
        "drupal/advagg": "^4.1",
        "drupal/antibot": "^1.4",
        "drupal/block_exclude_pages": "^2.0",
        "drupal/bootstrap": "^3.5",
        "drupal/cdn": "^3.1",
        "drupal/clientside_validation": "^3.0.0-rc3",
        "drupal/color_field": "^2.0-rc2",
        "drupal/config_filter": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/crop": "^2.1",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/elasticsearch_connector_autocomp": "^1.0",
        "drupal/email_registration": "1.1",
        "drupal/entityqueue": "^1.2",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/extrafield_views_integration": "^2.0",
        "drupal/facets": "^1.3",
        "drupal/field_group": "^3.2",
        "drupal/flag": "^4.0-alpha3",
        "drupal/flood_control": "^2.2",
        "drupal/flood_unblock": "^3.2",
        "drupal/group": "^1.0-rc1",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/mailchimp": "^2.0",
        "drupal/mailsystem": "^4.1",
        "drupal/maxlength": "^1.0@beta",
        "drupal/memcache": "^2.1",
        "drupal/metatag": "^1.4",
        "drupal/override_node_options": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.2",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_users": "^3.0",
        "drupal/redis": "^1.0@RC",
        "drupal/reroute_email": "^2.1",
        "drupal/rest_views": "^2.0",
        "drupal/restui": "^1.15",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/scheduler": "^1.0",
        "drupal/social_media": "^1.9-rc2",
        "drupal/swiftmailer": "^2.2",
        "drupal/system_status": "^2.8",
        "drupal/twig_tweak": "^2.9",
        "drupal/upgrade_status": "^3.14",
        "drupal/user_registrationpassword": "2.0-alpha1",
        "drupal/varnish_purge": "^2.1",
        "drupal/video_embed_field": "^2.4",
        "drupal/video_embed_html5": "1.2",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^1.4",
        "drush/drush": "^10.0",
        "ganlanyuan/tiny-slider": "master",
        "gdsmith/easing": "master",
        "itk-dev/web_accessibility_statement": "dev-master",
        "jquery_validation/jqueryvalidate": "~1.0",
        "kenwheeler/slick": "master",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/search_api_solr": {
                "grouping fix": "patches/search_api_solr/Issue-2900410-by-stdio-estoyausente-Error-when-using.patch"
            },
            "drupal/core": {
                "comment settings patch": "patches/core/comment_settings_tabs.patch",
                "invalid property": "patches/core/invalid_property.patch"
            },
            "drupal/redis": {
                "Flood: Compatibility with Flood unblock": "https://www.drupal.org/files/issues/redis-flood_unblock_external_flood-2928007-2.patch"
            },
            "drupal/image_widget_crop": {
                "Fix floating point issue": "https://www.drupal.org/files/issues/2020-06-22/3153736-2.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/custom/*/composer.json"
            ]
        }
    }
}
