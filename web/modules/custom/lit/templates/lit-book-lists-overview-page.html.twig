{% for group, items in book_lists %}
  <div class="field--book-lists-overview">
    <div class="field--label">
      {{ group }}
    </div>
    <div class="field__items">
      {% for item in items %}
        <div class="field--item">
          {{ link(item.title, item.url) }}
        </div>
      {% endfor %}
    </div>
  </div>
{% endfor %}
